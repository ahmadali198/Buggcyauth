{
  "name": "google-express-oauth",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec npx babel-node src/index.js",
    "clean": "npx rimraf ./build && npx mkdirp ./build",
    "prebuild": "npm run clean",
   "build": "npx babel src -d ./build --ignore ./node_modules",
    "db:setup": "sequelize db:create",
    "db:migrate:up": "sequelize db:migrate",
    "db:migrate:down": "sequelize db:migrate:undo:all",
    "db:refresh": "npm run db:migrate:down && npm run db:migrate:up",
    "render-build": "npm run build && npx sequelize-cli db:migrate",
    "start": "node build/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/node": "^7.12.13",
    "@babel/plugin-proposal-optional-chaining": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/register": "^7.12.13",
    "@babel/runtime": "^7.12.13",
    "babel-eslint": "^10.1.0",
    "babel-node": "0.0.1-security",
    "bcrypt": "^6.0.0",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.41.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mkdirp": "^3.0.1",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "multer-storage-cloudinary": "^4.0.0",
    "nodemon": "^2.0.7",
    "passport": "^0.4.1",
    "passport-google-token": "^0.1.2",
    "pg": "^8.5.1",
    "sequelize": "^6.5.0",
    "rimraf": "^6.0.1",
    "sequelize-cli": "^6.2.0"
  },
  "engines": {
  "node": ">=18.0.0"
},
